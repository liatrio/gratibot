    "chance": "^1.1.8",
    "codecov": "^3.8.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
